{
  "name": "reddytalk-ai",
  "version": "1.0.0",
  "description": "ReddyTalk.ai - AI-powered medical receptionist with <500ms latency",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "test": "jest",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:load": "k6 run tests/load/concurrent-calls.js",
    "test:pipeline": "node testVoicePipeline.js",
    "build": "docker build -t reddytalk-ai .",
    "deploy:dev": "kubectl apply -k k8s/overlays/dev",
    "deploy:prod": "kubectl apply -k k8s/overlays/prod",
    "lint": "eslint src/ tests/",
    "format": "prettier --write src/ tests/",
    "mcp:start": "node src/mcp/server.js",
    "mcp:test": "echo '{\"method\":\"resources/list\"}' | node src/mcp/server.js"
  },
  "keywords": [
    "ai",
    "healthcare",
    "voice",
    "azure",
    "hipaa",
    "clinic"
  ],
  "author": "ReddyTalk.ai Team",
  "license": "PROPRIETARY",
  "dependencies": {
    "@azure/communication-calling": "^1.38.1",
    "@azure/communication-identity": "^1.3.1",
    "@azure/communication-phone-numbers": "^1.5.0",
    "@azure/identity": "^4.0.1",
    "@azure/keyvault-secrets": "^4.7.0",
    "@azure/openai": "^1.0.0-beta.7",
    "@azure/service-bus": "^7.9.4",
    "@azure/storage-blob": "^12.17.0",
    "@deepgram/sdk": "^4.11.2",
    "@fastify/cors": "^8.5.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/rate-limit": "^8.1.1",
    "@fastify/websocket": "^8.3.1",
    "@modelcontextprotocol/sdk": "^1.18.0",
    "@prisma/client": "^5.6.0",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "fastify": "^4.24.3",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "microsoft-cognitiveservices-speech-sdk": "^1.34.1",
    "openai": "^5.20.1",
    "opentelemetry": "^0.1.0",
    "pg": "^8.11.3",
    "pino": "^8.16.2",
    "pino-pretty": "^10.2.3",
    "prisma": "^5.6.0",
    "prom-client": "^15.0.0",
    "rate-limiter-flexible": "^7.3.0",
    "redis": "^4.6.10",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/node": "^20.8.10",
    "eslint": "^8.53.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "supertest": "^6.3.3",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
